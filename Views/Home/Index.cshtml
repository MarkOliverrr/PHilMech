@section Scripts {
    <script>
        $(document).ready(function() {
            // Financial Reports Section Filtering
            function initFinancialFilter() {
                var $dropdownButton = $('#financialReportsDropdown');
                var $filterItems = $('.financial-section-filter');
                var $sectionTables = $('.financial-section-table');

                console.log('Dropdown button found:', $dropdownButton.length);
                console.log('Filter items found:', $filterItems.length);
                console.log('Section tables found:', $sectionTables.length);

                if ($dropdownButton.length === 0 || $filterItems.length === 0 || $sectionTables.length === 0) {
                    console.log('Elements not found, retrying...');
                    setTimeout(initFinancialFilter, 500);
                    return;
                }

                // Function to filter tables
                function filterTables(selectedSection) {
                    console.log('Filtering tables for section:', selectedSection);
                    $sectionTables.each(function() {
                        var $tableSection = $(this);
                        var tableSectionType = $tableSection.attr('data-section');
                        
                        if (selectedSection === 'all') {
                            // Show all tables
                            $tableSection.css('display', '');
                            console.log('Showing table:', tableSectionType);
                        } else {
                            // Show only table matching the selected section - hide others
                            if (tableSectionType === selectedSection) {
                                $tableSection.css('display', '');
                                console.log('Showing table:', tableSectionType);
                            } else {
                                $tableSection.css('display', 'none');
                                console.log('Hiding table:', tableSectionType);
                            }
                        }
                    });
                }

                // Handle dropdown item clicks - use event delegation
                $(document).off('click', '.financial-section-filter').on('click', '.financial-section-filter', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    
                    var selectedSection = $(this).attr('data-section');
                    var buttonText = $(this).text().trim();
                    
                    console.log('Section selected:', selectedSection);
                    
                    // If "All Sections", show "Select Section" instead
                    if (selectedSection === 'all') {
                        buttonText = 'Select Section';
                    }
                    
                    // Update dropdown button text
                    $dropdownButton.text(buttonText);
                    
                    // Filter table sections - this will hide other tables
                    filterTables(selectedSection);

                    // Close Bootstrap dropdown using jQuery
                    $dropdownButton.dropdown('hide');
                });

                // Initialize: Show all tables by default when page loads
                filterTables('all');
                console.log('Financial reports filter initialized successfully');
            }

            // Try to initialize
            setTimeout(initFinancialFilter, 500);

            // Projects Section (IV) Year Filtering
            function initProjectsFilter() {
                var $dropdownButton = $('#projectsDropdown');
                var $filterItems = $('.projects-year-filter');
                var $yearTables = $('.projects-year-table');

                if ($dropdownButton.length === 0 || $filterItems.length === 0 || $yearTables.length === 0) {
                    setTimeout(initProjectsFilter, 500);
