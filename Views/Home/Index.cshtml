                }

                function filterProjectsTables(selectedYear) {
                    $yearTables.each(function() {
                        var $tableSection = $(this);
                        var tableYear = $tableSection.attr('data-year');
                        
                        if (selectedYear === 'all') {
                            $tableSection.css('display', '');
                        } else {
                            if (tableYear === selectedYear) {
                                $tableSection.css('display', '');
                            } else {
                                $tableSection.css('display', 'none');
                            }
                        }
                    });
                }

                $(document).off('click', '.projects-year-filter').on('click', '.projects-year-filter', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    
                    var selectedYear = $(this).attr('data-year');
                    var buttonText = $(this).text().trim();
                    
                    if (selectedYear === 'all') {
                        buttonText = 'Select Year';
                    }
                    
                    $dropdownButton.text(buttonText);
                    filterProjectsTables(selectedYear);
                    $dropdownButton.dropdown('hide');
                });

                filterProjectsTables('all');
            }

